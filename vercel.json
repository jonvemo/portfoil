{
	"headers": [
		{
			"source": "/assets/fonts/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000"
				}
			]
		},
		{
			"source": "/assets/img/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000"
				}
			]
		},
		{
			"source": "/assets/icons/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000"
				}
			]
		},
		{
			"source": "/assets/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=2592000"
				}
			]
		},
		{
			"source": "/page/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=2592000"
				}
			]
		},
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Content-Security-Policy",
					"value": "frame-ancestors 'self';"
				},
				{
					"key": "Strict-Transport-Security",
					"value": "max-age=31536000; includeSubDomains; preload"
				},
				{
					"key": "Cache-Control",
					"value": "public, max-age=2592000"
				}
			]
		}
	]
}
